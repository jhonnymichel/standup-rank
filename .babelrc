{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          ">0.25%",
          "not ie 11",
          "not op_mini all"
        ]
      },
      "modules": false
    }],
    "react"
  ],
  "plugins": [
    ["transform-object-rest-spread", {"useBuiltIns": true}],
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}